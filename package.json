{
  "name": "tic-tac-toe",
  "version": "0.0.0",
  "description": "progressive, multiplayer tic tac toe, made with choo",
  "main": "src/app.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev --ignore-module normalize.css",
    "copy": "mkdir -p dist && cp src/index.html dist/",
    "dev": "npm run copy && budo src/app.js --open --dir=dist --live --pushstate -- ",
    "build": "npm run copy && NODE_ENV=production browserify src/app.js | uglifyjs -c > dist/app.js ",
    "test": "standard --verbose | snazzy && npm run deps"
  },
  "browserify": {
    "transform": [
      "sheetify/transform",
      "babelify"
    ]
  },
  "keywords": [
    "choo",
    "pwa"
  ],
  "author": "YerkoPalma",
  "license": "ISC",
  "dependencies": {
    "choo": "^3.0.2",
    "normalize.css": "^4.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "budo": "^8.3.0",
    "choo-log": "^1.4.0",
    "dependency-check": "^2.5.3",
    "insert-css": "^1.0.0",
    "sheetify": "^5.0.3",
    "standard": "^7.1.2",
    "uglify-js": "^2.7.0"
  }
}
