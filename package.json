{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "private": true,
  "description": "progressive, multiplayer tic tac toe, made with choo",
  "main": "server.js",
  "scripts": {
    "deps": "dependency-check . --entry index.js && dependency-check . --extra --no-dev --ignore-module tachyons --entry index.js",
    "start": "node server.js",
    "test": "standard --verbose | snazzy && npm run deps"
  },
  "browserify": {
    "transform": [
      "sheetify/transform"
    ]
  },
  "keywords": [
    "choo",
    "pwa"
  ],
  "standard": {
    "ignore": [
      "workbox-*",
      "sw.js"
    ]
  },
  "author": "YerkoPalma",
  "license": "MIT",
  "dependencies": {
    "bankai": "^9.1.0",
    "choo": "^6.6.1",
    "choo-service-worker": "^2.3.1",
    "nanocomponent": "^6.5.0",
    "tachyons": "^4.9.1"
  },
  "devDependencies": {
    "choo-devtools": "^2.3.2",
    "dependency-check": "^2.5.3",
    "sheetify": "^6.2.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "workbox": "0.0.0",
    "workbox-cli": "^2.1.2",
    "yo-yoify": "^4.3.0"
  }
}
